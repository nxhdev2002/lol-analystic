{
  "$defs": {
    "MatchEndedData": {
      "description": "Data structure for MatchEnded event.",
      "properties": {
        "match_id": {
          "description": "Riot match ID",
          "title": "Match Id",
          "type": "string"
        },
        "puuid": {
          "description": "Player PUUID",
          "title": "Puuid",
          "type": "string"
        },
        "summoner_name": {
          "description": "Summoner name",
          "title": "Summoner Name",
          "type": "string"
        },
        "game_duration": {
          "description": "Game duration in seconds",
          "title": "Game Duration",
          "type": "integer"
        },
        "win": {
          "description": "True if player won the match",
          "title": "Win",
          "type": "boolean"
        },
        "champion": {
          "description": "Champion played",
          "title": "Champion",
          "type": "string"
        },
        "kills": {
          "default": 0,
          "description": "Number of kills",
          "title": "Kills",
          "type": "integer"
        },
        "deaths": {
          "default": 0,
          "description": "Number of deaths",
          "title": "Deaths",
          "type": "integer"
        },
        "assists": {
          "default": 0,
          "description": "Number of assists",
          "title": "Assists",
          "type": "integer"
        },
        "kda": {
          "default": 0.0,
          "description": "KDA ratio",
          "title": "Kda",
          "type": "number"
        }
      },
      "required": [
        "match_id",
        "puuid",
        "summoner_name",
        "game_duration",
        "win",
        "champion"
      ],
      "title": "MatchEndedData",
      "type": "object"
    }
  },
  "description": "Event published when a match ends.",
  "properties": {
    "event_type": {
      "default": "match.ended",
      "title": "Event Type",
      "type": "string"
    },
    "timestamp": {
      "description": "Event timestamp",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    },
    "event_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Unique event identifier (UUID)",
      "title": "Event Id"
    },
    "correlation_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Correlation ID for tracing across services",
      "title": "Correlation Id"
    },
    "producer": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Service name that produced the event",
      "title": "Producer"
    },
    "data": {
      "$ref": "#/$defs/MatchEndedData"
    }
  },
  "required": [
    "data"
